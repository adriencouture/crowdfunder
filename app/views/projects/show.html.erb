<h1><%= @project.title %></h1>

<%= @project.title %><br>
<%= @project.description %><br>
<%= @project.end_date %><br>
$<%= @project.goal %><br>
<%= @project.backer_count %><br>
<%= @project.countdown %>
<br>


<% @project.rewards.order(amount: :asc).each do |reward| %>
<p>
  $<%= reward.amount %> <%= reward.title %>
</p>
<p>
  <%= reward.description %>
</p><br>
<% end %>
